const sendHttpRequest = require('sendHttpRequest');
const setCookie = require('setCookie');
const parseUrl = require('parseUrl');
const JSON = require('JSON');
const getRequestHeader = require('getRequestHeader');
const encodeUriComponent = require('encodeUriComponent');
const getCookieValues = require('getCookieValues');
const getEventData = require('getEventData');
const eventData = require('getAllEventData')();
const log = require('logToConsole');
const getContainerVersion = require('getContainerVersion');
const getType = require('getType');
const makeString = require('makeString');
const makeNumber = require('makeNumber');
const Math = require('Math');
const getTimestampMillis = require('getTimestampMillis');
const makeTableMap = require('makeTableMap');

const currentTime = Math.round(getTimestampMillis() / 1000);
const requestUrl = "https://api.awin.com/s2s/advertiser/" + data.advertiserId + "/orders";
const referrer = eventData.page_referrer || getRequestHeader('referer');
const url = eventData.page_location || eventData.page_referrer || getRequestHeader('referer');
const host = getRequestHeader('host');
const searchParams = parseUrl(url).searchParams;
const api_key = data.apiKey;
const channel = getChannel();

const channelCookie = getCookieValues('aw_ch')[0];
const clickTimeCookie = getCookieValues('aw_ct')[0];
const publisherIdCookie = getCookieValues('aw_affid')[0];
const awcCookie = getCookieValues('awc')[0];

if(data.eventType == "page_view") {
  
  const cookiesOptions = {
      domain: 'auto',
      path: '/',
      secure: true,
      httpOnly: true,
      'max-age': 31536000 // 365 days
  };
  
  if(channel != channelCookie && channel != 'aw') {
    setCookie('aw_ch', channel, cookiesOptions);
  }
  
  if(channel != channelCookie) {
    setCookie('aw_ct', makeString(currentTime), cookiesOptions);  
  }

  if(searchParams.awc) {
    setCookie('awc', searchParams.awc, cookiesOptions);
  }
  
  if(searchParams.aw_affid) {
    setCookie('aw_affid', searchParams.aw_affid, cookiesOptions);
  }
  
  data.gtmOnSuccess();
 
} else if(data.eventType == "conversion") {
  
  let requestBody = { 
    orders: [{
      orderReference: getOrderReference(),
      amount: getAmount(),
      channel: channelCookie || 'aw',
      awc: getAwc(),
      publisherId: getPublisherId(),
      clickTime: undefined,
      voucher: getVoucher(),
      currency: getCurrency(),
      transactionTime: currentTime,
      customerAcquisition: getCustomerAcquisition(),
      commissionGroups: getCommissionGroups(),
      custom: getCustomParameters(),
      basket: getBasket()    
    }],
    
  };
  
  if(clickTimeCookie && publisherIdCookie) {
    requestBody.orders[0].publisherId = getPublisherId();
    requestBody.orders[0].clickTime = getClickTime();
  }
  
  if(data.webhookUrl) {
    requestBody.webhook = { url: data.webhookUrl };
  }
  
  if(data.orderDataList) {
    data.orderDataList.forEach(d => {
      if(d.name == "amount" || d.name == "clickTime" || d.name == "transactionTime" || d.name == "publisherId") {
        requestBody.orders[0][d.name] = makeNumber(d.value);
      } else {
        requestBody.orders[0][d.name] = d.value;
      }
    });
  }
  
  sendConversionRequest(requestBody);

}


function sendConversionRequest(requestBody) {
  sendHttpRequest(
    requestUrl,
    (statusCode, headers, body) => {
      if (statusCode >= 200 && statusCode < 300) {
        data.gtmOnSuccess();
      } else {
        data.gtmOnFailure();
      }
    },
    { 
      method: 'POST', 
      headers: { 
        'x-api-key': api_key,
        'content-type': 'application/json'
      }
    },
    JSON.stringify(requestBody)
  );
}

function getOrderReference() {
  return eventData.transaction_id;
}

function getClickTime() {
  return makeNumber(clickTimeCookie);
}

function getChannel() {
  let channel = "aw";
  const referrerDomain = getDomain(referrer);
  const hostDomain = getDomain(host);
  log('referrer ' + referrerDomain);
  log('hostDomain ' + hostDomain);
  if(referrerDomain != hostDomain) {
    if(referrerDomain && referrerDomain.indexOf('awin') == -1) {
      channel = referrerDomain.split('.')[0];
    }
  }
  
  if(searchParams.gclid) {
    channel = "google";
  }
  
  if(searchParams.fbclid) {
    channel = "facebook";
  }
  
  if(searchParams.utm_source) {
    channel = searchParams.utm_source;
  }
  
  if(searchParams.source) {
    channel = searchParams.source;
  }
  
  if(channel.indexOf('awin') != -1) {
    channel = 'aw';
  }
  
  return channel;
}


function getAmount() {
  let amount = eventData.value;
  if(eventData.tax) {
    amount = amount - eventData.tax;
  }
  if(eventData.shipping) {
    amount = amount - eventData.shipping;
  }
  return makeDouble(amount);
}

function getAwc() {  
  return awcCookie;
}

function getPublisherId() {
  return makeNumber(publisherIdCookie);
}

function getVoucher() {
  return eventData.coupon;
}

function getCurrency() {
  return eventData.currency;
}

function getCustomerAcquisition() {
  let customerAcquisition;
  if(eventData.new_customer) {
    if(eventData.new_customer == "true") {
      customerAcquisition = "NEW";
    } else {
      customerAcquisition = "RETURNING";
    }
  }
  return customerAcquisition;
}

function getCommissionGroups() {
  let commissionGroups;
  
  if(data.commissionGroupsList) {
    commissionGroups = [];
    data.commissionGroupsList.forEach(commissionGroup => {
      commissionGroups.push({
        code: commissionGroup.code,
        amount: makeNumber(commissionGroup.amount)
      });
    });
  }
  
  return commissionGroups;
}

function getCustomParameters() {  
  return data.customParametersList ? makeTableMap(data.customParametersList, 'key', 'value') : undefined;
}

function getBasket() {
  let basket;
  let itemsArray = data.itemsArray || eventData.items;
  if(itemsArray && itemsArray.length > 0) {
    basket = [];
    itemsArray.forEach(item => {
      basket.push({
        id: item[getItemField('item_id')],
        name: item[getItemField('item_name')],
        price: makeNumber(item[getItemField('price')]),
        quantity: makeNumber(item[getItemField('quantity')]),
        commissionGroupCode: item[getItemField('commission_group')] || "DEFAULT",
        category: item[getItemField('item_category')],
        sku: item[getItemField('item_sku')]
      });
    });
  }
  
  return basket;
}

function getItemField(defaultField) {
  let result = defaultField;

  const map = {
    item_id: 'item_id',
    item_name: 'item_name',
    price: 'price',
    quantity: 'quantity',
    item_category: 'item_category',
    item_sku: 'item_sku',
  };

  if (data.itemDataList) {
    data.itemDataList.forEach(d => {
      if (map[defaultField] === d.name) {
        result = d.field;
      }
    });
  }

  return result;
}

/****** UTILS ******/

function makeDouble(value) {
    let str = makeString(value);
    if (str.indexOf(".") === -1) {
        return makeNumber(str + ".00");
    }
    let parts = str.split(".");
    return makeNumber(parts[0] + "." + (parts[1] + "00").substring(0, 2));
}

function getDomain(url) {
  if(url.indexOf('https://') != -1) {
    url = parseUrl(url).hostname;
  }
  let parts = url.split(".");
  if (parts.length > 2) {
    return parts.slice(-2).join(".");
  }
  return url;
}


